<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="demo.a.b.mapper.MemberDao">

<select id="login" parameterType="demo.a.b.dto.MemberDto" resultType="demo.a.b.dto.MemberDto">
  SELECT *
  FROM MEM
  WHERE ID = #{id}
  AND PWD = #{pwd}
</select>

<select id="regi" parameterType="demo.a.b.dto.MemberDto">
  INSERT INTO MEM(ID, PWD, EMAIL, NAME, ROLE)
  VALUES(#{id}, #{pwd}, #{email}, #{name}, #{role})
</select>

<select id="findByUserName" resultType="com.example.demo.auth.PrincipalDetails">
   SELECT *
   FROM MEM
   WHERE ID = #{username}
</select>

</mapper>